{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "MERN shopping card app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "seed": "node backend/seeder",
    "build": "npm install --prefix frontend && npm run build --prefix frontend",
    "lint": "eslint . --ext .js,.jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natnaelh14/ecommerce.git"
  },
  "author": "Natnael Haile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natnaelh14/ecommerce/issues"
  },
  "homepage": "https://github.com/natnaelh14/ecommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.12"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off"
    }
  }
}
